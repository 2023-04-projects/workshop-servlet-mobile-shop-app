<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>workshop-servlet-mobile-shop-app</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>stockaddpage1</servlet-name>
		<servlet-class>com.khadri.stock.mobile.servlet.StockAddPageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>stockaddpage1</servlet-name>
		<url-pattern>/stockaddpage</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>stockadd1</servlet-name>
		<servlet-class>com.khadri.stock.mobile.servlet.StockAddServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>stockadd1</servlet-name>
		<url-pattern>/stockadd</url-pattern>
	</servlet-mapping>
	
</web-app>